{
    "name": "Pixell/newsletter-voxmail-bundle",
    "type": "symfony-bundle",
    "description": "Simple bundle for Newsletter registration",
    "keywords": ["Voxmail", "newsletter"],
    "homepage": "https://antoninocarella.com",
    "license": "LGPL-3.0",
    "authors": [
        {
            "name": "Antonino Carella",
            "email": "antonio.carella@gmail.com"
        }
    ],
    "require": {
        "php": ">=5.3.8",
        "symfony/symfony": "~2.3",
        "doctrine/orm": "~2.2,>=2.2.3",
        "phpxmlrpc/phpxmlrpc":"*"
    },
    "require-dev": {
        "phpunit/phpunit": "4.*"
    },
    "config": {
        "bin-dir": "bin"
    },
    "autoload": {
        "psr-4": { "Pixell\\NewsletterBundle\\Voxmail\\": "" }
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.0.x-dev"
        }
    }
}